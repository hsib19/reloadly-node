{
  "name": "reloadly-node",
  "version": "1.0.1",
  "description": "Unofficial: A custom Node.js SDK for Reloadly API, supporting Airtime, GiftCards, and Utility Payments with TypeScript type-safety and per-service token management.",
  "private": false,
  "keywords": [
    "reloadly",
    "sdk",
    "nodejs",
    "typescript",
    "api",
    "payments",
    "topup"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:cov": "vitest --coverage"
  },
  "author": "Hasib Muharam",
  "license": "MIT",
  "packageManager": "pnpm@10.25.0",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "4.0.16",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsib19/reloadly-node.git"
  },
  "bugs": {
    "url": "https://github.com/hsib19/reloadly-node/issues"
  },
  "homepage": "https://github.com/hsib19/reloadly-node#readme"
}
